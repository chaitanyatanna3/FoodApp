<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chaitanya.foodapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ContactUsExample"
            android:label="Contact Us" />
        <activity android:name=".WebsiteExample"
            android:label="Web Site" />
        <activity android:name=".PreferenceExample"
            android:label="Preferences"
            android:configChanges="orientation"/>
        <activity android:name=".GalleryExample"
            android:label="Gallery" />
        <activity android:name=".MenuListExample"
            android:label="Menu" />
    </application>

</manifest>
